visible: false,\n      // 文本内容\n      content: \"\"\n    }\n  },\n  methods: {\n    // 显示弹框\n    show() {\n      this.visible = true\n    },\n    /** 创建按钮操作 */\n    handleCreateTable() {\n      if (this.content === \"\") {\n        this.$modal.msgError(\"请输入建表语句\")\n        return\n      }\n      createTable({ sql: this.content }).then(res => {\n        this.$modal.msgSuccess(res.msg)\n        if (res.code === 200) {\n          this.visible = false\n          this.$emit(\"ok\")\n        }\n      })\n    }\n  }\n}\n</script>\n"]}]}